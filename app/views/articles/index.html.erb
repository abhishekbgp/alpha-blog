<h1> listing all articles</h1>
<p>
  <%= link_to "create_new_article",new_article_path %>
</p>
<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
  </tr>
  
  <% @articles.each do |article| %>
  <tr>
    <td><%=article.title%></td>
    <td><%= article.description%>s</td>
    <td> <%= link_to 'edit',edit_article_path(article) %></td>
    <td> <%= link_to 'show',article_path(article)%></td>
    <td> <%= link_to 'delete',article_path(article), method: :delete,data: {confirm: "are u sure?"} %></td>
  </tr>
   <% end%> 
</table>